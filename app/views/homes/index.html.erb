<style>
.slick-slide {
    margin: 0px 20px;
}

.slick-slide img {
    width: 100%;
}

.slick-slider
{
    position: relative;
    display: block;
    box-sizing: border-box;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
            user-select: none;
    -webkit-touch-callout: none;
    -khtml-user-select: none;
    -ms-touch-action: pan-y;
        touch-action: pan-y;
    -webkit-tap-highlight-color: transparent;
}

.slick-list
{
    position: relative;
    display: block;
    overflow: hidden;
    margin: 0;
    padding: 0;
}
.slick-list:focus
{
    outline: none;
}
.slick-list.dragging
{
    cursor: pointer;
    cursor: hand;
}

.slick-slider .slick-track,
.slick-slider .slick-list
{
    -webkit-transform: translate3d(0, 0, 0);
       -moz-transform: translate3d(0, 0, 0);
        -ms-transform: translate3d(0, 0, 0);
         -o-transform: translate3d(0, 0, 0);
            transform: translate3d(0, 0, 0);
}

.slick-track
{
    position: relative;
    top: 0;
    left: 0;
    display: block;
}
.slick-track:before,
.slick-track:after
{
    display: table;
    content: '';
}
.slick-track:after
{
    clear: both;
}
.slick-loading .slick-track
{
    visibility: hidden;
}

.slick-slide
{
    display: none;
    float: left;
    height: 100%;
    min-height: 1px;
}
[dir='rtl'] .slick-slide
{
    float: right;
}
.slick-slide img
{
    display: block;
}
.slick-slide.slick-loading img
{
    display: none;
}
.slick-slide.dragging img
{
    pointer-events: none;
}
.slick-initialized .slick-slide
{
    display: block;
}
.slick-loading .slick-slide
{
    visibility: hidden;
}
.slick-vertical .slick-slide
{
    display: block;
    height: auto;
    border: 1px solid transparent;
}
.slick-arrow.slick-hidden {
    display: none;
}
.dynagraph {
  text-decoration: underline #0088CE;
}

.text-center.home {
    padding-top: 60px;
    padding-bottom: 60px;
}
img.dynagraph-grand {
    padding-top: 20px;
    padding-bottom: 20px;
}

section.homeview {
    padding-bottom: 50px!important;
}

.savoirfaire {
    padding-bottom: 50px!important;
}

[data-overlay] *:not(.container):not(.background-image-holder) {
    font-weight:bold;
}
[data-overlay="2"]:before {
    opacity: 0.5;
}
@media screen and (max-width: 940px) {
  a.feature img:first-child:not(:last-child) {
    min-height: 51px!important;
}
}

.test2{
    position: absolute;
    top: 0;
    left: 0;
}
.h3 {
    font-weight:bold;
}
.switchable__text div {
    font-size: 16px!important;
    color: grey!important;
}

.test{
      font-size: 1.35714286em!important;
    line-height: 1.68421053em!important;
        font-weight: 400!important;
    color: #808080!important;
}
img.slick-slide.slick-active {
    width: auto;
    height:auto;
    max-height: 90px;
}
.slick-track {
    display: flex!important;
    align-items: center!important;
}
.background-image-holder {
opacity: 1; object-fit: cover;
}
.image.grand {
    padding-top: 20px;
    padding-bottom: 20px;
    margin: auto;
}
.image.grand img {
    width:30em;
}
section.customer-logos.slider.slick-initialized.slick-slider {
    max-height:93px!important;
}

.feature img {
        min-height: 196px;
}

@media screen and (max-width: 1200px) {
.feature img {
        min-height: 158px;
}
}
@media screen and (max-width: 940px) {
.feature img {
        min-height:auto;
}
}
</style>
