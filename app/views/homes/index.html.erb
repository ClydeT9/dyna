<script>
function createSlick(){  

	$(".customer-logos").not('.slick-initialized').slick({
    autoplay: true,
    dots: false,
    arrows:false,
    pauseOnHover: false,
    slidesToShow: 7,
    slidesToScroll: 1,
    autoplaySpeed: 1500,
    responsive: [{ 
        breakpoint: 500,
        settings: {
            dots: false,
            arrows: false,
            infinite: false,
            slidesToShow: 2,
            slidesToScroll: 2
        } 
    }]
});	

}

createSlick();

//Now it will not throw error, even if called multiple times.
$(window).on( 'resize', createSlick );
</script>